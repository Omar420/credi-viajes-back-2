{
  "name": "back-credit-viajes",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "dev-ts": "tsc --watch",
    "dev": "tsc && yarn run copy-public && yarn run copy-html && nodemon dist/app",
    "dev-local": "tsc && nodemon dist/app",
    "start": "tsc && yarn run copy-public && yarn run copy-html && node dist/app",
    "migrations:run": "sequelize-cli db:migrate",
    "seed:run": "sequelize-cli db:seed:all",
    "copy-html": "mkdir -p ./dist/src/email/templates && cp -r ./src/email/templates/. ./dist/src/email/templates",
    "copy-public": "mkdir -p ./dist/public && cp -r ./public/. ./dist/public"
  },
  "keywords": [],
  "author": "Vilo Technologies",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "globals": "^15.10.0",
    "sequelize-cli": "^6.6.2",
    "tslint": "^6.1.3",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "jwt": "^0.2.0",
    "module-alias": "^2.2.3",
    "moment-timezone": "^0.5.48",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pg": "^8.14.1",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "twilio": "^5.5.2",
    "umzug": "^3.8.2",
    "uuid": "^11.1.0"
  },
  "_moduleAliases": {
    "@src": "dist/src"
  },
  "packageManager": "yarn@4.9.2"
}
